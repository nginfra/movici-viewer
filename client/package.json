{
  "name": "movici-viewer",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.ts,.js --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ckpack/vue-color": "^1.5.0",
    "@deck.gl/core": "^8.9.35",
    "@luma.gl/core": "^8.5.21",
    "@placemarkio/geo-viewport": "^1.0.1",
    "@nebula.gl/layers": "^1.0.4",
    "@nebula.gl/overlays": "^1.0.4",
    "@oruga-ui/oruga-next": "^0.11.4",
    "@oruga-ui/theme-bulma": "^0.7.3",
    "axios": "^1.7.7",
    "bulma": "^1.0.2",
    "bulma-utilities": "^0.3.6",
    "chart.js": "^4.4.6",
    "chartjs-plugin-annotation": "^3.1.0",
    "geotiff": "^2.1.3",
    "lodash-es": "^4.17.21",
    "mapbox-gl": "^3.8.0",
    "pinia": "^2.2.8",
    "proj4": "^2.12.1",
    "reproject": "^1.2.7",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.1",
    "vue-i18n": "^10.0.6",
    "vue-router": "^4.5.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@danmarshall/deckgl-typings": "^4.9.28",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/mapbox-gl": "^3.4.0",
    "@types/node": "^22.11.10",
    "@types/proj4": "^2.5.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^2.1.6",
    "@vitest/ui": "^2.1.6",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.31.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.3.3",
    "sass": "^1.82.0",
    "typescript": "^5.6.3",
    "unplugin-auto-import": "^0.18.6",
    "vite": "^5.4.11",
    "vitest": "^2.1.6",
    "vue-tsc": "^2.1.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "packageManager": "npm@11.4.2"
}